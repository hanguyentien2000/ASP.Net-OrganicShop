
@{
    ViewBag.Title = "UsersInf";
    Layout = "~/Views/Shared/_LayoutNoLeftSideBar.cshtml";
    LeafShop.Models.KhachHang db = (LeafShop.Models.KhachHang)HttpContext.Current.Session[LeafShop.Session.ConstaintUser.USER_SESSION];

}

<section id="content">
    <div class="content-wrap">
        <div class="container clearfix">
            <div class="fancy-title title-bottom-border">
                <h3>Đơn hàng đã đặt</h3>
            </div>
            <div class="table-responsive account-table">

                <p>Bạn chưa đặt mua sản phẩm</p>

            </div>
            <div class="fancy-title title-bottom-border">
                <h3>Thông tin tài khoản</h3>
            </div>
           
            <div class='account-detail'>
                @foreach (var item in Model)
                {

                    <h4>@item.TenKhachHang</h4>


                    <p>@item.TenDangNhap</p>

                    <p></p>

                    <p></p>

                    <p> @item.DiaChi</p>
                    <p></p>

                    <p><a id='view_address' href='/account/addresses'>Xem địa chỉ</a></p>
                }
                </div>
            

        </div>
    </div>
</section>
